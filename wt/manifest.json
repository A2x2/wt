{

    "manifest_version": 2,
    "name": "WT",
    "version": "1.0",
  
    "description": "Wavetapper",
  
    "icons": {
      "48": "icons/hs-48.png"
    },

    "background": {
      "scripts": ["background.js"]
    },
  
    "browser_action": {
      "default_icon": {
        "16": "icons/page-16.png",
        "32": "icons/page-32.png"
      } 
    },

    "permissions": [
      "<all_urls>",
      "tabs"
    ]
  }